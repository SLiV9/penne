fn main()
{
	var a = 55;
	var b = 66;
	var c = 77;
	var x: []&i32 = [&a, &b];
	// [&a, &b], a = 55, b = 66, c = 77
	x[0] = 22;
	// [&a, &b], a = 22, b = 66, c = 77
	&x[0] = &c;
	// [&c, &b], a = 22, b = 66, c = 77
	x[0] = x[1];
	// [&c, &b], a = 22, b = 66, c = 66
	&x[0] = &x[1];
	// [&b, &b], a = 22, b = 66, c = 66
	c = 200;
	// [&b, &b], a = 22, b = 66, c = 200
	x[0] = 15;
	// [&b, &b], a = 22, b = 15, c = 200
	var result = x[0] + x[1];
	// result = 30
	return: result
}
