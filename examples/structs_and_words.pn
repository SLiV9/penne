word64 Position
{
	x: i32,
	y: i32,
}

fn sum(pos: Position) -> i32
{
	return: pos.x + pos.y
}

word128 Line
{
	from: Position,
	to: Position,
}

fn sum_of_end_points(line: Line) -> i32
{
	var result = sum(line.from) + sum(line.to);
	return: result
}

fn main() -> i32
{
	var from = Position { x: 10, y: 10 };
	var line = Line { from, to: Position { x: 10, y: 0 } };
	line.to.y = 170;
	var result = sum_of_end_points(line);
	return: result
}
