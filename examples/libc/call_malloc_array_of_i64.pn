import "vendor:libc/stdlib.pn";

fn main() -> u8
{
	var result = -1;
	var ptr: &[10]i64 = malloc(10 * |:i64|) as &[10]i64;
	if &ptr == 0x0
		goto after_free;
	var i = 0;
	{
		if i == 10
			goto done;
		if i == 23
			goto error_occurred;
		ptr[i] = i as i64;
		i = i + 1;
		loop;
	}
	done:
	result = ptr[5];
	error_occurred:
	free(&ptr as &[...]u8);
	after_free:
	return: result as u8
}
