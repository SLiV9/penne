word64 Position
{
	x: i32,
	y: i32,
}

struct Foo
{
	a: u8,
	x: i32,
}

struct Bar
{
	x: i128,
	foo: Foo,
	pos: Position,
}

struct Baz
{
	a: u8,
	x: i128,
	foo: Foo,
}

const SIZE_OF_BAZ: usize = |:Baz|;

word32 Padded
{
	a: u8,
	b: u16,
}

fn main() -> usize
{
	var total = |:Position| + |:Foo| + |:Bar| + |:Baz| + |:Padded|;
	var expected =    2 * 4 +  2 * 4 +  4 * 8 +  4 * 8 +     2 * 2;
	return: 168 + SIZE_OF_BAZ + total - expected
}
