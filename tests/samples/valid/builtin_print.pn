
fn main() -> u8
{
	hello_world:
	print!("Hello world!\n");
	with_name:
	var name = "Alice";
	print!("Hello ", name, "!\n");
	with_newline:
	var newline = "\n";
	print!("Hello a", newline, "b!", newline);
	with_integer:
	var x: i32 = 1337;
	with_pointer:
	var ptr: &i32 = 0x7f00;
	print!("Hello ", &ptr, "!\n");
	return: 0
}
