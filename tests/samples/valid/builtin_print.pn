
fn main() -> u8
{
	hello_world:
	print!("Hello world!\n");
	with_name:
	var name = "Alice";
	print!("Hello ", name, "!\n");
	with_newline:
	print!("Hello a", '\n', "b!", '\n');
	with_byte:
	var b: u8 = 255;
	print!("Hello ", b, "!\n");
	with_integer:
	var x: i32 = -173;
	print!("Hello ", x, "!\n");
	with_bool:
	var is_ok = true;
	print!("Hello ", is_ok, "!\n");
	with_inline_value:
	print!("Hello ", false, "!\n");
	with_usize:
	print!("Hello ", 32000usize, "!\n");
	with_pointer:
	var ptr: &i32 = 0x7f00;
	print!("Hello ", &ptr, "!\n");
	with_c_string:
	var world_with_nul = "world\0";
	var cstr: &[...]char8 = cast &world_with_nul;
	print!("Hello ", cstr, "!\n");
	return: 0
}
